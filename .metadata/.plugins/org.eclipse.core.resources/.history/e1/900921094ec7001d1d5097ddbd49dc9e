package JavaClass;

public class methodchaining {

	public void m1()
	{
		System.out.println("M1 is calling--- ");
	}
	
	public void m2()
	{
		System.out.println("M1 is calling--- ");
	}
	
	public void m3()
	{
		System.out.println("M1 is calling--- ");
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		methodchaining c1= new methodchaining();
		c1.m1().m2;
	}

}
